{
  "builds": [
    { "src": "restaurant_app/app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "restaurant_app/app.py" }
  ],
  "functions": {
    "restaurant_app/app.py": {
      "includeFiles": [
        "restaurant_app/templates/**",
        "restaurant_app/static/**",
        "restaurant_app/banco_integration.py"
      ]
    }
  }
}
